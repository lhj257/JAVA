package qwe;

public class Qwe {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String city = "서울";
		String country="";
		switch(city) {
		case "서울":
		case "부산": country="한국";
		case "북경": country="중국";
		case "동경": country="일본"; break;
		default:country="대상없음";
		}
			System.out.println(country);
		}
	}


